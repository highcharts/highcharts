{
    "compilerOptions": {
        "target": "ES2020",
        "module": "Preserve",
        "lib": [],
        "allowJs": true,
        "isolatedModules": true,
        "moduleDetection": "force",
        "resolveJsonModule": true,
        "strict": false,
        "moduleResolution": "Node",
        "baseUrl": "./",
        "paths": {
            "highcharts": [
                "../../code/highcharts.src"
            ],
            "highcharts/*": [
                "../../code/*.src"
            ]
        },
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
