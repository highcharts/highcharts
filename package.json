{
  "name": "highcharts",
  "version": "11.4.8",
  "private": true,
  "description": "JavaScript charting framework",
  "module": "es-modules/masters/highcharts.src.js",
  "author": "Highsoft AS <support@highcharts.com> (http://www.highcharts.com/about)",
  "repository": {
    "type": "git",
    "url": "https://github.com/highcharts/highcharts.git"
  },
  "bugs": "https://github.com/highcharts/highcharts/issues",
  "homepage": "http://www.highcharts.com",
  "keywords": [
    "charts",
    "graphs",
    "visualization",
    "data",
    "browserify",
    "webpack"
  ],
  "scripts": {
    "api": "gulp api",
    "dapi": "gulp dashboards/api",
    "dapi2": "gulp dashboards/api2",
    "dbuild": "gulp dashboards/dist",
    "dgulp": "gulp dashboards/scripts-watch",
    "dtest": "gulp dashboards/test",
    "dlint": "gulp dashboards/lint",
    "test": "npx gulp test-ts --force && npx gulp test --force --speak",
    "test-node": "tsx ./test/ts-node-unit-tests/index.ts",
    "test-node:watch": "tsx ./test/ts-node-unit-tests/index.ts --watch",
    "test:precommit": "npx gulp test-ts --modified && npx gulp test --modified && npm run test-node",
    "testall": "npx gulp test --browsers all --force --speak",
    "test-all": "npx gulp test --browsers all --force --speak",
    "test-dts": "npx gulp scripts && npx gulp jsdoc-dts && npx gulp lint-dts",
    "test-tskarma": "gulp test-karma",
    "build": "npx gulp dist",
    "jsdoc": "npx gulp jsdoc-watch --skip-websearch",
    "jsdoc-highcharts": "npx gulp jsdoc-watch --products highcharts --skip-websearch",
    "jsdoc-highstock": "npx gulp jsdoc-watch --products highstock --skip-websearch",
    "lint": "cd ts && npx eslint",
    "ts-compile:test": "npx tsc -p test && npx tsc -p samples",
    "gulp": "npx gulp",
    "utils": "highcharts-utils",
    "prepare": "husky install",
    "postinstall": "npx gulp update-vendor && npm update browserstack-node-sdk",
    "reset": "npx gulp reset-clone && npm i",
    "benchmark": "cd test/ts-node-unit-tests && node -r ts-node/register bench.ts",
    "benchmark-compare": "cd test/ts-node-unit-tests && node -r ts-node/register benchmark-compare.ts",
    "test:precommit-browserstack": "browserstack-node-sdk npx gulp test-ts --modified && npx gulp test --modified && npx ts-node ./test/ts-node-unit-tests",
    "benchmark-browserstack": "cd test/ts-node-unit-tests && browserstack-node-sdk node -r ts-node/register bench.ts",
    "benchmark-compare-browserstack": "cd test/ts-node-unit-tests && browserstack-node-sdk node -r ts-node/register benchmark-compare.ts"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.592.0",
    "@aws-sdk/credential-providers": "^3.592.0",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@cypress-audit/lighthouse": "^1.4.2",
    "@highcharts/highcharts-assembler": "github:highcharts/highcharts-assembler#v1.5.6",
    "@highcharts/highcharts-declarations-generator": "github:highcharts/highcharts-declarations-generator#v1.2.0",
    "@highcharts/highcharts-documentation-generators": "github:highcharts/highcharts-documentation-generators#v0.6.9",
    "@highcharts/map-collection": "^2.2.0",
    "@octokit/rest": "^20.1.1",
    "@playwright/test": "^1.45.3",
    "@swc/core": "^1.5.25",
    "@types/jquery": "^3.5.30",
    "@types/node": "^20.14.12",
    "@types/qunit": "^2.19.10",
    "@types/trusted-types": "^2.0.7",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-eslint": "^10.1.0",
    "browserify": "^17.0.0",
    "browserstack-node-sdk": "^1.34.10",
    "bundle-declarations-webpack-plugin": "^5.1.1",
    "chatgpt": "^5.2.5",
    "cloudflare": "^3.2.0",
    "colors": "^1.4.0",
    "cypress": "^13.11.0",
    "cypress-visual-regression": "3.0.0",
    "eslint": "^8.57.0",
    "eslint-config-eslint": "7.0.0",
    "eslint-plugin-brackets": "^0.1.3",
    "eslint-plugin-highcharts": "github:highcharts/eslint-plugin-highcharts#v1.0.9",
    "eslint-plugin-jsdoc": "^48.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^2.1.1",
    "fs-extra": "^11.2.0",
    "gifencoder": "^2.0.1",
    "glob": "^10.4.1",
    "gulp": "^5.0.0",
    "gulp-jsdoc3": "^3.0.0",
    "gulp-zip": "5.1.0",
    "gzip-size": "6.0.0",
    "highsoft-websearch": "github:highcharts/highsoft-websearch#v0.0.3",
    "husky": "^8.0.3",
    "jquery": "^3.7.1",
    "js-yaml": "3.14.1",
    "jsdom": "^21.1.0",
    "jspdf": "2.5.1",
    "karma": "^6.4.3",
    "karma-browserstack-launcher": "^1.6.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^2.1.3",
    "karma-generic-preprocessor": "^1.1.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-qunit": "^4.2.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sharding": "^4.4.0",
    "lint-staged": "^15.2.5",
    "lolex": "^6.0.0",
    "marked": "^4.2.12",
    "moment": "2.30",
    "moment-timezone": "^0.5.45",
    "node-fetch": "^2.6.12",
    "plugin-error": "^2.0.1",
    "pngjs": "^6.0.0",
    "pretty": "^2.0.0",
    "proj4": "2.3.6",
    "qunit": "^2.21.0",
    "request": "^2.88.2",
    "requirejs": "^2.3.7",
    "semver": "^7.6.2",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^29.0.0",
    "svg2pdf.js": "2.2.3",
    "topojson-client": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typedoc": "^0.25.13",
    "typedoc-plugin-rename-defaults": "^0.7.0",
    "typescript": "~5.4.5",
    "undertaker-forward-reference": "^2.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": ">=14.14.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "./ts/!(Dashboards|DataGrid)**/*.ts": [
      "npm run lint"
    ],
    "./ts/Dashboards/**/*.ts": [
      "cd ts/Dashboards && npx eslint"
    ],
    "./ts/DataGrid/**/*.ts": [
      "cd ts/DataGrid && npx eslint"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "license": "SEE LICENSE IN <license.txt>"
}
