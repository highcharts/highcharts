{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "composite": false,
        "declaration": true,
        "baseUrl": "./",
        "moduleResolution": "node",
        "outDir": "../../js/",
        "rootDir": "../",
        "paths": {
            "highcharts": [
                "../Dashboards/highcharts"
            ],
            "@highcharts/dashboards/datagrid": [
                "../Dashboards/datagrid"
            ]
        }
    },
    "exclude": [
        "../Dashboards/**/*.ts",
        "../masters/**/*.ts",
        "../masters-dashboards/**/*.ts",
        "../masters-grid/**/*.ts",
        "../masters-es5/**/*.ts"
    ],
    "include": [
        "../**/*.ts",
        "../masters/modules/accessibility.src.ts",
    ]
}
